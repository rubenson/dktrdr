	Eprint = {http://archpsyc.ama-assn.org/cgi/reprint/62/5/529.pdf},
	Journal = {Arch Gen Psychiatry},
	Number = {5},
	Pages = {529-535},
	Title = {The Interaction of Stressful Life Events and a Serotonin Transporter Polymorphism in the Prediction of Episodes of Major Depression: A Replication},
	Url = {http://archpsyc.ama-assn.org/cgi/content/abstract/62/5/529},
	Volume = {62},
	Year = {2005},

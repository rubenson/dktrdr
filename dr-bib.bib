@article{Baybeck_McClurg:05,
	Abstract = {The connection between the individual and his or her aggregate geographic environment, usually defined as the neighborhood, is a key component of the contextual model of social influence. However, there is substantial anecdotal evidence that people have very little knowledge or connection to their neighborhood. In this article, the authors explore the connection by using data from the South Bend Study to answer two questions: What do people know about objective conditions of their neighborhoods? Second, do these conditions influence perceptions people have of their neighborhoods?The authors find that (a) people have a good deal of knowledge about the objective conditions, and (b) even after controlling for individual factors, these conditions positively influence how people perceive their status within the neighborhood. In short, there is a connection between the individual and the geographic context.
},
	Author = {Baybeck, Brady and McClurg, Scott D.},
	Date-Added = {2017-06-16 10:04:54 +0000},
	Date-Modified = {2017-06-16 10:04:55 +0000},
	Eprint = {http://apr.sagepub.com/cgi/reprint/33/4/492.pdf},
	Journal = APR,
	Number = {4},
	Pages = {492--520},
	Title = {{What Do They Know and How Do They Know It?: An Examination of Citizen Awareness of Context}},
	Url = {http://apr.sagepub.com/cgi/content/abstract/33/4/492},
